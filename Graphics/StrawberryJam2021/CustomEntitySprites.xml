<?xml version="1.0" encoding="utf-8" ?>

<Sprites>

    <!--Bubble Emitter-->
    <bubbleEmitter path="objects/StrawberryJam2021/bubbleEmitter/" start="idle">
        <Loop id="idle" path="close" delay="0.1" frames="3"/>
        <Anim id="open" path="open" delay="0.1" frames="0-1" goto="close"/>
        <Anim id="close" path="close" delay="0.1" frames="0-2" goto="idle"/>
    </bubbleEmitter>

    <bubble path="objects/StrawberryJam2021/bubbleEmitter/bubble/" start="appear">
        <Anim id="appear" path="appear" delay="0.1" frames="0-2" goto="idle"/>
        <Loop id="idle" path="idle" delay="0.1" frames="0"/>
        <Anim id="pop" path="pop" delay="0.01" frames="0-4"/>
    </bubble>

    <!--Grab Temple Gate-->
    <grabTempleGate path="objects/StrawberryJam2021/grabTempleGate/" start="idle">
        <Justify x="0.5" y="0"/>
        <Loop id="idle" path="TempleDoor" delay="0.1" frames="0"/>
        <Anim id="open" path="TempleDoor" delay="0.1" frames="6-14" goto="idle"/>
        <Anim id="hit" path="TempleDoor" delay="0.08" frames="0-4" goto="idle"/>
    </grabTempleGate>

    <!--Horizontal Temple Gate-->
    <horizontalTempleGateLeft path="objects/StrawberryJam2021/horizontalTempleGate/left/" start="idle">
        <Justify x="0" y="0.25"/>
        <Loop id="idle" path="TempleDoor" delay="0.1" frames="0"/>
        <Anim id="open" path="TempleDoor" delay="0.1" frames="6-14" goto="idle"/>
        <Anim id="hit" path="TempleDoor" delay="0.08" frames="0-4" goto="idle"/>
    </horizontalTempleGateLeft>

    <horizontalTempleGateRight path="objects/StrawberryJam2021/horizontalTempleGate/right/" start="idle">
        <Justify x="0" y="0.25"/>
        <Loop id="idle" path="TempleDoor" delay="0.1" frames="0"/>
        <Anim id="open" path="TempleDoor" delay="0.1" frames="6-14" goto="idle"/>
        <Anim id="hit" path="TempleDoor" delay="0.08" frames="0-4" goto="idle"/>
    </horizontalTempleGateRight>

    <!--Exploding Strawberry-->
    <explodingStrawberry path="objects/StrawberryJam2021/explodingStrawberry/" start="idle">
        <Center/>
        <Anim id="idle" path="explodeberry" delay="0.1" frames="0"/>
        <Anim id="explode" path="explodeberry" delay="0.1" frames="0-11"/>
        <Anim id="ghostexplode" path="explodeghostberry" delay="0.1" frames="0-11"/>
    </explodingStrawberry>

    <!--Sky Lantern-->
    <skyLantern path="objects/StrawberryJam2021/skyLantern/" start="idle">
        <Justify x="0.5" y="0.9"/>
        <Anim id="idle" path="idle" delay="0.1"/>
        <Anim id="held" path="held" delay="0.1"/>
        <Anim id="fall" path="fall" delay="0.06" goto="fallLoop"/>
        <Loop id="fallLoop" path="fallLoop" delay="0.06"/>
        <Anim id="death" path="death" delay="0.06"/>
    </skyLantern>

    <!--Wormhole Booster-->
    <wormholeBooster path="objects/StrawberryJam2021/boosterWormhole/" start="loop">
        <Justify x="0.5" y="0.5"/>
        <Loop id="loop" path="boosterWormhole" delay="0.1" frames="0-4"/>
        <Loop id="inside" path="boosterWormhole" delay="0.1" frames="5-8"/>
        <Loop id="spin" path="boosterWormhole" delay="0.06" frames="18-25"/>
        <Anim id="pop" path="boosterWormhole" delay="0.08" frames="9-17"/>
    </wormholeBooster>

    <wormholeMask path="objects/StrawberryJam2021/boosterDisplacementMask/" start="loop">
        <Justify x="0.5" y="0.5"/>
        <Loop id="loop" path="boosterWormhole" delay="0.1" frames="0-4"/>
        <Loop id="inside" path="boosterWormhole" delay="0.1" frames="5-8"/>
        <Loop id="spin" path="boosterWormhole" delay="0.06" frames="18-25"/>
        <Anim id="pop" path="boosterWormhole" delay="0.08" frames="9-17"/>
    </wormholeMask>

    <!--Laser Emitter-->
    <laserEmitter path="objects/StrawberryJam2021/laserEmitter/" start="simple">
        <Justify x="0.5" y="1"/>
        <Anim id="simple" path="simple" delay="0.1" frames="0"/>
        <Anim id="rounded_base" path="rounded_base" delay="0.1" frames="0"/>
        <Anim id="rounded_tint" path="rounded_tint" delay="0.1" frames="0"/>
    </laserEmitter>

    <!--Brush Laser Emitter-->
    <brushLaserEmitter path="objects/StrawberryJam2021/brushLaserEmitter/" start="pink_idle">
        <Justify x="0" y="0.5"/>
        <Anim id="pink_idle" path="pink/brush-a/brush-a-idle" delay="0.1" frames="0"/>
        <Anim id="pink_charging" path="pink/brush-a/brush-a-charging" delay="0.1" frames="0-7"/>
        <Anim id="pink_burst" path="pink/brush-a/brush-a-burst" delay="0.1" frames="0,1" goto="pink_firing"/>
        <Loop id="pink_firing" path="pink/brush-a/brush-a-firing" delay="0.1" frames="0-3"/>
        <Anim id="pink_cooldown" path="pink/brush-a/brush-a-cooldown" delay="0.1" frames="0-3" goto="pink_idle"/>
        <Anim id="blue_idle" path="blue/brush-a/brush-a-idle" delay="0.1" frames="0"/>
        <Anim id="blue_charging" path="blue/brush-a/brush-a-charging" delay="0.1" frames="0-7"/>
        <Anim id="blue_burst" path="blue/brush-a/brush-a-burst" delay="0.1" frames="0,1" goto="blue_firing"/>
        <Loop id="blue_firing" path="blue/brush-a/brush-a-firing" delay="0.1" frames="0-3"/>
        <Anim id="blue_cooldown" path="blue/brush-a/brush-a-cooldown" delay="0.1" frames="0-3" goto="blue_idle"/>
    </brushLaserEmitter>

    <!-- Brush Laser Paint: Appears behind the brush while it is firing -->
    <brushLaserPaintBack path="objects/StrawberryJam2021/brushLaserEmitter/" start="pink_cooldown">
        <Justify x="0" y="0.5"/>
        <Loop id="pink_firing" path="pink/wide/brush-a-glow" delay="0.1" frames="11-14"/>
        <Anim id="pink_cooldown" path="pink/wide/brush-a-glow" delay="0.1" frames="15-17"/>
        <Loop id="blue_firing" path="blue/wide/brush-a-glow" delay="0.1" frames="11-14"/>
        <Anim id="blue_cooldown" path="blue/wide/brush-a-glow" delay="0.1" frames="15-17"/>
    </brushLaserPaintBack>

    <!--Brush Laser Small Brushes: Appears between large brushes in wide emitters -->
    <brushLaserEmitterSmall path="objects/StrawberryJam2021/brushLaserEmitter/" start="pink_a">
        <Justify x="0" y="0.5"/>
        <Anim id="pink_a" path="pink/wide/backbrushA" delay="0.1" frames="0"/>
        <Anim id="pink_b" path="pink/wide/backbrushB" delay="0.1" frames="0"/>
        <Anim id="pink_c" path="pink/wide/backbrushC" delay="0.1" frames="0"/>
        <Anim id="pink_d" path="pink/wide/backbrushD" delay="0.1" frames="0"/>
        <Anim id="blue_a" path="blue/wide/backbrushA" delay="0.1" frames="0"/>
        <Anim id="blue_b" path="blue/wide/backbrushB" delay="0.1" frames="0"/>
        <Anim id="blue_c" path="blue/wide/backbrushC" delay="0.1" frames="0"/>
        <Anim id="blue_d" path="blue/wide/backbrushD" delay="0.1" frames="0"/>
    </brushLaserEmitterSmall>

    <!--Brush Laser Beam-->
    <brushLaserBeam path="objects/StrawberryJam2021/brushLaserEmitter/" start="pink_burst">
        <Justify x="0" y="0.5"/>
        <Anim id="pink_burst" path="pink/beam-burst" delay="0.1" frames="0,1" goto="pink_firing"/>
        <Loop id="pink_firing" path="pink/beam-firing" delay="0.1" frames="0-3"/>
        <Anim id="blue_burst" path="blue/beam-burst" delay="0.1" frames="0,1" goto="blue_firing"/>
        <Loop id="blue_firing" path="blue/beam-firing" delay="0.1" frames="0-3"/>
    </brushLaserBeam>

    <!--Wide Brush Laser Beam-->
<!--    <wideBrushLaserBeam path="objects/StrawberryJam2021/brushLaserEmitter/" start="pink_burst">-->
<!--        <Justify x="0" y="0.5"/>-->
<!--        <Anim id="pink_burst" path="pink/wide/beam-burst" delay="0.1" frames="0,1" goto="pink_firing"/>-->
<!--        <Loop id="pink_firing" path="pink/wide/beam-firing" delay="0.1" frames="0-3"/>-->
<!--        <Anim id="blue_burst" path="blue/wide/beam-burst" delay="0.1" frames="0,1" goto="blue_firing"/>-->
<!--        <Loop id="blue_firing" path="blue/wide/beam-firing" delay="0.1" frames="0-3"/>-->
<!--    </wideBrushLaserBeam>-->

    <!--Flag Breaker Box-->
    <flagBreakerBox path="objects/StrawberryJam2021/flagBreakerBox/" start="idle">
        <Justify x="0.5" y="0.5"/>
        <Loop id="idle" path="idle" delay="0.1"/>
        <Anim id="open" path="open" delay="0.06" frames="1-3" goto="opened"/>
        <Loop id="opened" path="opened" delay="0.06"/>
        <Anim id="break" path="break" delay="0.06"/>
    </flagBreakerBox>

    <!--Rose Glider-->
    <roseGlider path="objects/StrawberryJam2021/roseGlider/" start="idle_3">
        <Anim id="idle_3" path="3charge/fly" delay="0.1" frames="3-7,6-4" goto="idle_3:9,idle_gust_3:1"/>
        <Anim id="idle_gust_3" path="3charge/fly" delay="0.1" frames="3-0,1,2" goto="idle_3:9,idle_gust_3:1"/>
        <Anim id="idle_2" path="2charge/fly" delay="0.1" frames="3-7,6-4" goto="idle_2:9,idle_gust_2:1"/>
        <Anim id="idle_gust_2" path="2charge/fly" delay="0.1" frames="3-0,1,2" goto="idle_2:9,idle_gust_2:1"/>
        <Anim id="idle_1" path="1charge/fly" delay="0.1" frames="3-7,6-4" goto="idle_1:9,idle_gust_1:1"/>
        <Anim id="idle_gust_1" path="1charge/fly" delay="0.1" frames="3-0,1,2" goto="idle_1:9,idle_gust_1:1"/>
        <Anim id="death" path="death" delay="0.1" />
    </roseGlider>

    <!--Strawberry Jam Jars-->
    <jamJar_trailer path="objects/StrawberryJam2021/jamJar/trailer/" start="empty">
        <Justify x="0.5" y="1"/>
        <Loop id="empty" path="jarempty_idle" delay="0.1"/>
        <Anim id="before_fill" path="jarempty_idle" delay="0.1" frames="8-15" goto="fill"/>
        <Anim id="fill" path="jarfill_" delay="0.1" goto="full"/>
        <Loop id="full" path="jarfull_idle" delay="0.1"/>
    </jamJar_trailer>
</Sprites>
